     showPersons={this.state.showPersons}\n                    personsLength={this.state.persons.length}\n                    clicked={this.togglePersonsHandler}\n                />\n                    ): null}\n\n\n                {persons}\n\n\n                <br />\n            </Aux>\n            // After nesting for a long time it becomes too much. Better to not use this way.\n            //   React.createElement('div', {className: 'App'}, React.createElement('h1', null, 'testing'))\n\n        );\n    }\n}\n\nexport default withClass(App, classes.App);\n","/Users/jason/IdeaProjects/react-exercises/src/Components/Persons/Persons.js",[],"/Users/jason/IdeaProjects/react-exercises/src/Components/Persons/Person/Person.js",["47"],"/Users/jason/IdeaProjects/react-exercises/src/Components/Cockpit/Cockpit.js",[],"/Users/jason/IdeaProjects/react-exercises/src/hoc/Aux.js",[],"/Users/jason/IdeaProjects/react-exercises/src/hoc/WithClass.js",[],{"ruleId":"48","replacedBy":"49"},{"ruleId":"50","replacedBy":"51"},{"ruleId":"48","replacedBy":"52"},{"ruleId":"50","replacedBy":"53"},{"ruleId":"54","severity":1,"message":"55","line":2,"column":8,"nodeType":"56","messageId":"57","endLine":2,"endColumn":15},{"ruleId":"54","severity":1,"message":"58","line":5,"column":8,"nodeType":"56","messageId":"57","endLine":5,"endColumn":17},{"ruleId":"54","severity":1,"message":"55","line":2,"column":8,"nodeType":"56","messageId":"57","endLine":2,"endColumn":15},"no-native-reassign",["59"],"no-negated-in-lhs",["60"],["59"],["60"],"no-unused-vars","'classes' is defined but never used.","Identifier","unusedVar","'withClass' is defined but never used.","no-global-assign","no-unsafe-negation"]